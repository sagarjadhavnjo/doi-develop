	<queryString>
		<![CDATA[SELECT
    bpn.BPN_NAME,
    bpn.BPN_NAME_GUJ,
    tpnh.MINISTER_NAME,
    tpnh.MINISTER_NAME_GUJ,
    final_query.MAJORHEAD_CODE,  
    final_query.MAJORHEAD_NAME,
    final_query.MAJORHEAD_CODE_GUJ,
    final_query.MAJORHEAD_NAME_GUJ,
    final_query.SUBMAJORHEAD_CODE,
    final_query.SUBMAJORHEAD_NAME,
    final_query.SUBMAJORH_CODE_GUJ,
    final_query.SUBMAJORH_NAME_GUJ,
    final_query.MINORHEAD_CODE,
    final_query.MINORHEAD_NAME,
    final_query.MINORHEAD_CODE_GUJ,
    final_query.MINORHEAD_NAME_GUJ,
    final_query.SUBHEAD_CODE,
    final_query.SUBHEAD_NAME,
    final_query.SUBHEAD_CODE_GUJ,
    final_query.SUBHEAD_NAME_GUJ,
    GU.EN AS NUMOFGRANT_ENG,  
    tpnh.ITEM_NAME,
    GU.GUJ AS NUMOFGRANT_GUJ,
    tpnh.ITEM_NAME_GUJ,
    final_query.REC_AMT,
    final_query.NON_REC_AMT,
    final_query.ULT_ANN_REC_AMT,
    tpnh.WRITE_UP,
    tpnh.WRITE_UP_GUJ,
    final_query.TBUD_PUB_HDR_ID  
FROM
(
SELECT
    ROW_NUMBER() OVER() AS NO,
    main_table.MAJORHEAD_CODE,
    main_table.MAJORHEAD_CODE_GUJ,
    main_table.MAJORHEAD_NAME,
    main_table.MAJORHEAD_NAME_GUJ,
    main_table.SUBMAJORHEAD_CODE,
    main_table.SUBMAJORH_CODE_GUJ,
    main_table.SUBMAJORHEAD_NAME,
    main_table.SUBMAJORH_NAME_GUJ,
    main_table.MINORHEAD_CODE,
    main_table.MINORHEAD_CODE_GUJ,
    main_table.MINORHEAD_NAME,
    main_table.MINORHEAD_NAME_GUJ,
    main_table.SUBHEAD_CODE,
    main_table.SUBHEAD_CODE_GUJ,
    main_table.SUBHEAD_NAME,
    main_table.SUBHEAD_NAME_GUJ,
    main_table.REC_AMT,
    main_table.NON_REC_AMT,
    main_table.ULT_ANN_REC_AMT,
    main_table.TBUD_PUB_HDR_ID
FROM
(
SELECT
    first_set.MAJORHEAD_CODE,
    first_set.MAJORHEAD_CODE_GUJ,
    first_set.MAJORHEAD_NAME,
    first_set.MAJORHEAD_NAME_GUJ,
    first_set.SUBMAJORHEAD_CODE,
    first_set.SUBMAJORH_CODE_GUJ,
    first_set.SUBMAJORHEAD_NAME,
    first_set.SUBMAJORH_NAME_GUJ,
    first_set.MINORHEAD_CODE,
    first_set.MINORHEAD_CODE_GUJ,
    first_set.MINORHEAD_NAME,
    first_set.MINORHEAD_NAME_GUJ,
    first_set.SUBHEAD_CODE,
    first_set.SUBHEAD_CODE_GUJ,
    first_set.SUBHEAD_NAME,
    first_set.SUBHEAD_NAME_GUJ,
    sum(first_set.REC_AMT) AS REC_AMT,
    sum(first_set.NON_REC_AMT) AS NON_REC_AMT,
    sum(first_set.ULT_ANN_REC_AMT) AS ULT_ANN_REC_AMT,
    first_set.TBUD_PUB_HDR_ID  
FROM
(  
SELECT
    mm.MAJORHEAD_CODE,
    REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(mm.MAJORHEAD_CODE_GUJ,'1','૧'), '2', '૨'),'3','૩'),
    '4','૪'),'5','૫'),'6','૬'),'7','૭'),'8','૮'),'9','૯'),'0','૦') AS MAJORHEAD_CODE_GUJ,
    mm.MAJORHEAD_NAME,
    mm.MAJORHEAD_NAME_GUJ,
    msmh.SUBMAJORHEAD_CODE,
    REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(msmh.SUBMAJORH_CODE_GUJ,'1','૧'), '2', '૨'),'3','૩'),
    '4','૪'),'5','૫'),'6','૬'),'7','૭'),'8','૮'),'9','૯'),'0','૦') AS SUBMAJORH_CODE_GUJ,
    msmh.SUBMAJORHEAD_NAME,
    msmh.SUBMAJORH_NAME_GUJ,
    mmh.MINORHEAD_CODE,
    REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(mmh.MINORHEAD_CODE_GUJ,'1','૧'), '2', '૨'),'3','૩'),
    '4','૪'),'5','૫'),'6','૬'),'7','૭'),'8','૮'),'9','૯'),'0','૦') AS MINORHEAD_CODE_GUJ,
    mmh.MINORHEAD_NAME,
    mmh.MINORHEAD_NAME_GUJ,
    ms.SUBHEAD_CODE,
    REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(ms.SUBHEAD_CODE_GUJ,'1','૧'), '2', '૨'),'3','૩'),
    '4','૪'),'5','૫'),'6','૬'),'7','૭'),'8','૮'),'9','૯'),'0','૦') AS SUBHEAD_CODE_GUJ,
    ms.SUBHEAD_NAME,
    ms.SUBHEAD_NAME_GUJ,
    tpd.RECURRENT_AMOUNT AS REC_AMT,
    tpd.NON_RECURRENT_AMOUNT AS NON_REC_AMT,
    ROUND(tpd.ULTIMATE_ANNUAL_RECURRENT_AMOUNT) AS ULT_ANN_REC_AMT,
    tph.TBUD_PUB_HDR_ID
FROM
    BUDGET.TBUD_PUB_HDR tph,
    BUDGET.TBUD_PUB_DTL tpd,
    MASTER_V1.MS_MAJORHEAD mm,
    MASTER_V1.MS_SUBMAJORHEAD msmh,
    MASTER_V1.MS_MINORHEAD mmh,
    MASTER_V1.MS_SUBHEAD ms
WHERE
    tpd.TBUD_PUB_HDR_ID = tph.TBUD_PUB_HDR_ID
    AND tph.MAJORHEAD_ID = mm.MAJORHEAD_ID
    AND tph.SUBMAJORHEAD_ID = msmh.SUBMAJORHEAD_ID
    AND tph.MINORHEAD_ID = mmh.MINORHEAD_ID
    AND tph.SUBHEAD_ID = ms.SUBHEAD_ID
    AND tph.APP_TYPE_ID=426
    AND tph.BPN_ID = $P{bpnId}
    AND tph.FINANCIAL_YEAR_ID = $P{fyId}
ORDER BY
    mm.MAJORHEAD_CODE,
    msmh.SUBMAJORHEAD_CODE,
    mmh.MINORHEAD_CODE,
    ms.SUBHEAD_CODE) first_set
GROUP BY
    first_set.MAJORHEAD_CODE,
    first_set.MAJORHEAD_CODE_GUJ,
    first_set.MAJORHEAD_NAME,
    first_set.MAJORHEAD_NAME_GUJ,
    first_set.SUBMAJORHEAD_CODE,
    first_set.SUBMAJORH_CODE_GUJ,
    first_set.SUBMAJORHEAD_NAME,
    first_set.SUBMAJORH_NAME_GUJ,
    first_set.MINORHEAD_CODE,
    first_set.MINORHEAD_CODE_GUJ,
    first_set.MINORHEAD_NAME,
    first_set.MINORHEAD_NAME_GUJ,
    first_set.SUBHEAD_CODE,
    first_set.SUBHEAD_CODE_GUJ,
    first_set.SUBHEAD_NAME,
    first_set.SUBHEAD_NAME_GUJ,
    first_set.TBUD_PUB_HDR_ID) main_table
WHERE (main_table.REC_AMT > 50 OR main_table.NON_REC_AMT > 100)
ORDER BY
    main_table.MAJORHEAD_CODE,
    main_table.SUBMAJORHEAD_CODE,
    main_table.MINORHEAD_CODE,
    main_table.SUBHEAD_CODE) final_query
JOIN (SELECT ENG EN, GUJ FROM EDP.GUJNO ORDER BY ENG) GU ON final_query.NO = GU.EN
JOIN (SELECT BPN_NAME, BPN_NAME_GUJ, BPN_ID FROM MASTER_V1.MS_BPN) bpn ON bpn.BPN_ID=$P{bpnId}
JOIN
(
    SELECT
        tpnh2.TBUD_PUB_HDR_ID,
        tpnh2.ITEM_NAME,
        tpnh2.ITEM_NAME_GUJ,
        tpnh2.WRITE_UP,
        tpnh2.WRITE_UP_GUJ,
        mmic.MINISTER_NAME,
        mmic.MINISTER_NAME_GUJ
    FROM
        BUDGET.TBUD_PUB_NC_HDR tpnh2
        LEFT JOIN MASTER_V1.MS_MINISTER_IN_CHARGE mmic ON  tpnh2.MINISTER_IN_CHARGE = mmic.MS_MINI_IN_CHARGE_ID
   ) tpnh ON final_query.TBUD_PUB_HDR_ID = tpnh.TBUD_PUB_HDR_ID
ORDER BY final_query.NO]]>
	</queryString>
