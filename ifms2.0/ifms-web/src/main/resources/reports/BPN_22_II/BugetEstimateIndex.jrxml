	<queryString language="SQL">
		<![CDATA[SELECT
	count(*) OVER(PARTITION BY final_query.MAJORHEAD_CODE) AS majHeadCount,
	GU.GUJ AS NUMOFGRANT_GUJ,
	GU.EN AS NUMOFGRANT_ENG,
	final_query.ACC_ChaVotEng,
	final_query.ACC_ChaVotGuj,
	final_query.ACC_BUD_EST AS ACCOUNTS,
	final_query.MBE_ChaVotEng,
	final_query.MBE_ChaVotGuj,
	final_query.MOD_BUD_EST AS MODIFIED_BUDGET_ESTIMATES,
	final_query.RE_ChaVotEng,
	final_query.RE_ChaVotGuj,
	final_query.RE_BUD_EST AS REVIESED_ESTIMATES,
	final_query.BE_ChaVotEng,
	final_query.BE_ChaVotGuj,
	final_query.BUD_EST AS BUDGET_ESTIMATES,
	final_query.MAJORHEAD_CODE AS MAJORHEAD_CODE,
	final_query.MAJORHEAD_NAME,
	final_query.MAJORHEAD_NAME_GUJ,
	final_query.MAJORHEAD_CODE_GUJ,
	nextYear.FY AS BUD_EST_YEAR_ENG,
	nextYear.FY_GUJ AS BUD_EST_YEAR_GUJ,
	currYear.FY AS MOD_REV_YEAR_ENG,
	currYear.FY_GUJ AS MOD_REV_YEAR_GUJ,
	prevYear.FY AS ACC_YEAR_ENG,
	prevYear.FY_GUJ AS ACC_YEAR_GUJ
FROM
	(
	SELECT
		ROW_NUMBER() OVER() AS NO,
		main_table.MAJORHEAD_CODE,
		main_table.MAJORHEAD_NAME,
		main_table.MAJORHEAD_NAME_GUJ,
		main_table.MAJORHEAD_CODE_GUJ,
		main_table.ACC_BUD_EST,
		main_table.MOD_BUD_EST,
		main_table.RE_BUD_EST,
		main_table.BUD_EST,
		main_table.BE_ChaVotEng,
		main_table.BE_ChaVotGuj,
		main_table.MBE_ChaVotEng,
		main_table.MBE_ChaVotGuj,
		main_table.RE_ChaVotEng,
		main_table.RE_ChaVotGuj,
		main_table.ACC_ChaVotEng,
		main_table.ACC_ChaVotGuj
	FROM
		(
		SELECT
			COALESCE(NFY.MAJORHEAD_CODE,
			CFY1.MAJORHEAD_CODE,
			CFY2.MAJORHEAD_CODE,
			PFY.MAJORHEAD_CODE) AS MAJORHEAD_CODE,
			COALESCE(NFY.MAJORHEAD_NAME,
			CFY1.MAJORHEAD_NAME,
			CFY2.MAJORHEAD_NAME,
			PFY.MAJORHEAD_NAME) AS MAJORHEAD_NAME,
			COALESCE(NFY.MAJORHEAD_NAME_GUJ,
			CFY1.MAJORHEAD_NAME_GUJ,
			CFY2.MAJORHEAD_NAME_GUJ,
			PFY.MAJORHEAD_NAME_GUJ) AS MAJORHEAD_NAME_GUJ,
			COALESCE(NFY.MAJORHEAD_CODE_GUJ,
			CFY1.MAJORHEAD_CODE_GUJ,
			CFY2.MAJORHEAD_CODE_GUJ,
			PFY.MAJORHEAD_CODE_GUJ) AS MAJORHEAD_CODE_GUJ,
			PFY.ACC_BUD_EST,
			CFY1.MOD_BUD_EST,
			CFY2.RE_BUD_EST,
			NFY.BUD_EST,
			NFY.BE_ChaVotEng,
			NFY.BE_ChaVotGuj,
			CFY1.MBE_ChaVotEng,
			CFY1.MBE_ChaVotGuj,
			CFY2.RE_ChaVotEng,
			CFY2.RE_ChaVotGuj,
			PFY.ACC_ChaVotEng,
			PFY.ACC_ChaVotGuj
		FROM
			(
			SELECT
				abc.MAJORHEAD_CODE,
				ROUND(SUM(abc.BE)+ SUM(abc.state)+ SUM(abc.css), 2) AS BUD_EST,
				abc.MAJORHEAD_ID,
				abc.MAJORHEAD_NAME,
				abc.MAJORHEAD_CODE_GUJ,
				abc.MAJORHEAD_NAME_GUJ,
				abc.BE_ChaVotEng,
				abc.BE_ChaVotGuj
			FROM
				(
				SELECT
					MH.MAJORHEAD_CODE,
					DTL.PROPOSED_AMOUNT AS BE,
					0 AS state,
					0 AS css,
					HDR.MAJORHEAD_ID,
					MH.MAJORHEAD_NAME,
					MH.MAJORHEAD_CODE_GUJ,
					MH.MAJORHEAD_NAME_GUJ,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'Charged'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'Voted'
					END AS BE_ChaVotEng,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'ષબન-મતપાત્ર'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'મતપાત્ર'
					END AS BE_ChaVotGuj
				FROM
					MASTER_V1.MS_MAJORHEAD MH,
					BUDGET.TBUD_PUB_HDR HDR,
					BUDGET.TBUD_PUB_DTL DTL,
					MASTER_V1.MS_OBJECTHEAD MO
				WHERE
					HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
					AND DTL.OBJECT_ID = mo.OBJECT_ID
					AND HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
					AND MO.OBJECT_CODE IN ('3131',
					'3132',
					'3561')
					AND HDR.BPN_ID =  $P{bpnId} 
					AND HDR.FINANCIAL_YEAR_ID = $P{fyId}
					AND HDR.APP_TYPE_ID IN (425,
					426,
					427)
			UNION ALL
				SELECT
					MH.MAJORHEAD_CODE,
					0 AS BE,
					PA_STATE AS state,
					PA_CSS AS css,
					HDR.MAJORHEAD_ID,
					MH.MAJORHEAD_NAME,
					MH.MAJORHEAD_CODE_GUJ,
					MH.MAJORHEAD_NAME_GUJ,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'Charged'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'Voted'
					END AS BE_ChaVotEng,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'ષબન-મતપાત્ર'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'મતપાત્ર'
					END AS BE_ChaVotGuj
				FROM
					MASTER_V1.MS_MAJORHEAD MH,
					BUDGET.TBUD_PUB_HDR HDR,
					BUDGET.TBUD_PUB_NC_HDR DTL,
					MASTER_V1.MS_OBJECTHEAD MO
				WHERE
					HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
					AND DTL.OBJECT_ID = mo.OBJECT_ID
					AND HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
					AND MO.OBJECT_CODE IN ('3131',
					'3132',
					'3561')
					AND HDR.BPN_ID = $P{bpnId} 
					AND HDR.FINANCIAL_YEAR_ID = $P{fyId}
					AND HDR.APP_TYPE_ID IN (428,
					429) ) abc
			GROUP BY
				abc.MAJORHEAD_CODE,
				abc.MAJORHEAD_ID,
				abc.MAJORHEAD_NAME,
				abc.MAJORHEAD_CODE_GUJ,
				abc.MAJORHEAD_NAME_GUJ,
				abc.BE_ChaVotEng,
				abc.BE_ChaVotGuj ) NFY
		FULL OUTER JOIN (
			SELECT
				abc.MAJORHEAD_CODE,
				ROUND(SUM(abc.BE)+ SUM(abc.state)+ SUM(abc.css), 2) AS MOD_BUD_EST,
				abc.MAJORHEAD_ID,
				abc.MAJORHEAD_NAME,
				abc.MAJORHEAD_CODE_GUJ,
				abc.MAJORHEAD_NAME_GUJ,
				abc.MBE_ChaVotEng,
				abc.MBE_ChaVotGuj
			FROM
				(
				SELECT
					MH.MAJORHEAD_CODE,
					DTL.PROPOSED_AMOUNT AS BE,
					0 AS state,
					0 AS css,
					HDR.MAJORHEAD_ID,
					MH.MAJORHEAD_NAME,
					MH.MAJORHEAD_CODE_GUJ,
					MH.MAJORHEAD_NAME_GUJ,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'Charged'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'Voted'
					END AS MBE_ChaVotEng,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'ષબન-મતપાત્ર'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'મતપાત્ર'
					END AS MBE_ChaVotGuj
				FROM
					MASTER_V1.MS_MAJORHEAD MH,
					BUDGET.TBUD_PUB_HDR HDR,
					BUDGET.TBUD_PUB_DTL DTL,
					MASTER_V1.MS_OBJECTHEAD MO
				WHERE
					HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
					AND DTL.OBJECT_ID = mo.OBJECT_ID
					AND HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
					AND MO.OBJECT_CODE IN ('3131',
					'3132',
					'3561')
					AND HDR.BPN_ID = $P{bpnId} 
					AND HDR.FINANCIAL_YEAR_ID = $P{fyId} -1
					AND HDR.APP_TYPE_ID IN (425,
					426,
					427)
					AND HDR.IS_REVISED <> 1
			UNION ALL
				SELECT
					MH.MAJORHEAD_CODE,
					0 AS BE,
					PA_STATE AS state,
					PA_CSS AS css,
					HDR.MAJORHEAD_ID,
					MH.MAJORHEAD_NAME,
					MH.MAJORHEAD_CODE_GUJ,
					MH.MAJORHEAD_NAME_GUJ,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'Charged'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'Voted'
					END AS MBE_ChaVotEng,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'ષબન-મતપાત્ર'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'મતપાત્ર'
					END AS MBE_ChaVotGuj
				FROM
					MASTER_V1.MS_MAJORHEAD MH,
					BUDGET.TBUD_PUB_HDR HDR,
					BUDGET.TBUD_PUB_NC_HDR DTL,
					MASTER_V1.MS_OBJECTHEAD MO
				WHERE
					HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
					AND DTL.OBJECT_ID = mo.OBJECT_ID
					AND HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
					AND MO.OBJECT_CODE IN ('3131',
					'3132',
					'3561')
					AND HDR.BPN_ID = $P{bpnId} 
					AND HDR.FINANCIAL_YEAR_ID = $P{fyId} -1
					AND HDR.APP_TYPE_ID IN (428,
					429)
					AND HDR.IS_REVISED <> 1 ) abc
			GROUP BY
				abc.MAJORHEAD_CODE,
				abc.MAJORHEAD_ID,
				abc.MAJORHEAD_NAME,
				abc.MAJORHEAD_CODE_GUJ,
				abc.MAJORHEAD_NAME_GUJ,
				abc.MBE_ChaVotEng,
				abc.MBE_ChaVotGuj ) CFY1 ON
			CFY1.MAJORHEAD_ID = NFY.MAJORHEAD_ID
			AND NFY.BE_ChaVotEng = CFY1.MBE_ChaVotEng
		FULL OUTER JOIN (
				SELECT abc.MAJORHEAD_CODE,
				ROUND(SUM(abc.BE)+ SUM(abc.state)+ SUM(abc.css), 2) AS RE_BUD_EST,
				abc.MAJORHEAD_ID,
				abc.MAJORHEAD_NAME,
				abc.MAJORHEAD_CODE_GUJ,
				abc.MAJORHEAD_NAME_GUJ,
				abc.RE_ChaVotEng,
				abc.RE_ChaVotGuj
			FROM
				(
				SELECT
					MH.MAJORHEAD_CODE,
					DTL.PROPOSED_AMOUNT AS BE,
					0 AS state,
					0 AS css,
					HDR.MAJORHEAD_ID,
					MH.MAJORHEAD_NAME,
					MH.MAJORHEAD_CODE_GUJ,
					MH.MAJORHEAD_NAME_GUJ,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'Charged'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'Voted'
					END AS RE_ChaVotEng,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'ષબન-મતપાત્ર'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'મતપાત્ર'
					END AS RE_ChaVotGuj
				FROM
					MASTER_V1.MS_MAJORHEAD MH,
					BUDGET.TBUD_PUB_HDR HDR,
					BUDGET.TBUD_PUB_DTL DTL,
					MASTER_V1.MS_OBJECTHEAD MO
				WHERE
					HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
					AND DTL.OBJECT_ID = mo.OBJECT_ID
					AND HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
					AND MO.OBJECT_CODE IN ('3131',
					'3132',
					'3561')
					AND HDR.BPN_ID = $P{bpnId}
					AND HDR.FINANCIAL_YEAR_ID = $P{fyId} -1
					AND HDR.APP_TYPE_ID IN (425,
					426,
					427)
					AND HDR.IS_REVISED = 1
			UNION ALL
				SELECT
					MH.MAJORHEAD_CODE,
					0 AS BE,
					PA_STATE AS state,
					PA_CSS AS css,
					HDR.MAJORHEAD_ID,
					MH.MAJORHEAD_NAME,
					MH.MAJORHEAD_CODE_GUJ,
					MH.MAJORHEAD_NAME_GUJ,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'Charged'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'Voted'
					END AS RE_ChaVotEng,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'ષબન-મતપાત્ર'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'મતપાત્ર'
					END AS RE_ChaVotGuj
				FROM
					MASTER_V1.MS_MAJORHEAD MH,
					BUDGET.TBUD_PUB_HDR HDR,
					BUDGET.TBUD_PUB_NC_HDR DTL,
					MASTER_V1.MS_OBJECTHEAD MO
				WHERE
					HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
					AND DTL.OBJECT_ID = mo.OBJECT_ID
					AND HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
					AND MO.OBJECT_CODE IN ('3131',
					'3132',
					'3561')
					AND HDR.BPN_ID = $P{bpnId} 
					AND HDR.FINANCIAL_YEAR_ID = $P{fyId} -1
					AND HDR.APP_TYPE_ID IN (428,
					429)
					AND HDR.IS_REVISED = 1 ) abc
			GROUP BY
				abc.MAJORHEAD_CODE,
				abc.MAJORHEAD_ID,
				abc.MAJORHEAD_NAME,
				abc.MAJORHEAD_CODE_GUJ,
				abc.MAJORHEAD_NAME_GUJ,
				abc.RE_ChaVotEng,
				abc.RE_ChaVotGuj ) CFY2 ON
			CFY2.MAJORHEAD_ID = NFY.MAJORHEAD_ID
			AND NFY.BE_ChaVotEng = CFY2.RE_ChaVotEng
		FULL OUTER JOIN (
			SELECT
				abc.MAJORHEAD_CODE,
				ROUND(SUM(abc.BE)+ SUM(abc.state)+ SUM(abc.css), 2) AS ACC_BUD_EST,
				abc.MAJORHEAD_ID,
				abc.MAJORHEAD_NAME,
				abc.MAJORHEAD_CODE_GUJ,
				abc.MAJORHEAD_NAME_GUJ,
				abc.ACC_ChaVotEng,
				abc.ACC_ChaVotGuj
			FROM
				(
				SELECT
					MH.MAJORHEAD_CODE,
					DTL.PROPOSED_AMOUNT AS BE,
					0 AS state,
					0 AS css,
					HDR.MAJORHEAD_ID,
					MH.MAJORHEAD_NAME,
					MH.MAJORHEAD_CODE_GUJ,
					MH.MAJORHEAD_NAME_GUJ,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'Charged'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'Voted'
					END AS ACC_ChaVotEng,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'ષબન-મતપાત્ર'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'મતપાત્ર'
					END AS ACC_ChaVotGuj
				FROM
					MASTER_V1.MS_MAJORHEAD MH,
					BUDGET.TBUD_PUB_HDR HDR,
					BUDGET.TBUD_PUB_DTL DTL,
					MASTER_V1.MS_OBJECTHEAD MO
				WHERE
					HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
					AND DTL.OBJECT_ID = mo.OBJECT_ID
					AND HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
					AND MO.OBJECT_CODE IN ('3131',
					'3132',
					'3561')
					AND HDR.BPN_ID = $P{bpnId} 
					AND HDR.FINANCIAL_YEAR_ID = $P{fyId}
					AND HDR.APP_TYPE_ID IN (425,
					426,
					427)
			UNION ALL
				SELECT
					MH.MAJORHEAD_CODE,
					0 AS BE,
					PA_STATE AS state,
					PA_CSS AS css,
					HDR.MAJORHEAD_ID,
					MH.MAJORHEAD_NAME,
					MH.MAJORHEAD_CODE_GUJ,
					MH.MAJORHEAD_NAME_GUJ,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'Charged'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'Voted'
					END AS ACC_ChaVotEng,
					CASE
						WHEN HDR.IS_CHARGED_VOTED = 223 THEN 'ષબન-મતપાત્ર'
						WHEN HDR.IS_CHARGED_VOTED = 224 THEN 'મતપાત્ર'
					END AS ACC_ChaVotGuj
				FROM
					MASTER_V1.MS_MAJORHEAD MH,
					BUDGET.TBUD_PUB_HDR HDR,
					BUDGET.TBUD_PUB_NC_HDR DTL,
					MASTER_V1.MS_OBJECTHEAD MO
				WHERE
					HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
					AND DTL.OBJECT_ID = mo.OBJECT_ID
					AND HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
					AND MO.OBJECT_CODE IN ('3131',
					'3132',
					'3561')
					AND HDR.BPN_ID = $P{bpnId} 
					AND HDR.FINANCIAL_YEAR_ID = $P{fyId}
					AND HDR.APP_TYPE_ID IN (428,
					429) ) abc
			GROUP BY
				abc.MAJORHEAD_CODE,
				abc.MAJORHEAD_ID,
				abc.MAJORHEAD_NAME,
				abc.MAJORHEAD_CODE_GUJ,
				abc.MAJORHEAD_NAME_GUJ,
				abc.ACC_ChaVotEng,
				abc.ACC_ChaVotGuj ) PFY ON
			PFY.MAJORHEAD_ID = NFY.MAJORHEAD_ID
			AND NFY.BE_ChaVotEng = PFY.ACC_ChaVotEng
		ORDER BY
			MAJORHEAD_CODE ) main_table
	WHERE
		main_table.ACC_BUD_EST IS NOT NULL
		AND main_table.ACC_BUD_EST > 0
		OR (main_table.MOD_BUD_EST IS NOT NULL
		AND main_table.MOD_BUD_EST > 0)
		OR (main_table.RE_BUD_EST IS NOT NULL
		AND main_table.RE_BUD_EST > 0)
		OR (main_table.BUD_EST IS NOT NULL
		AND main_table.BUD_EST > 0) ) final_query
JOIN (
		SELECT ENG EN,
		GUJ
	FROM
		EDP.GUJNO
	ORDER BY
		ENG) GU ON
	final_query.NO = GU.EN
JOIN (
		SELECT mfy.FY,
		mfy.FY_GUJ,
		mfy.FINANCIAL_YEAR_ID
	FROM
		MASTER_V1.MS_FINANCIAL_YEAR mfy
	WHERE
		mfy.FINANCIAL_YEAR_ID = $P{fyId}) nextYear ON
	nextYear.FINANCIAL_YEAR_ID = $P{fyId} 
JOIN (
		SELECT mfy.FY,
		mfy.FY_GUJ,
		mfy.FINANCIAL_YEAR_ID
	FROM
		MASTER_V1.MS_FINANCIAL_YEAR mfy
	WHERE
		mfy.FINANCIAL_YEAR_ID = $P{fyId} -1) currYear ON
	currYear.FINANCIAL_YEAR_ID = $P{fyId} -1
JOIN (
		SELECT mfy.FY,
		mfy.FY_GUJ,
		mfy.FINANCIAL_YEAR_ID
	FROM
		MASTER_V1.MS_FINANCIAL_YEAR mfy
	WHERE
		mfy.FINANCIAL_YEAR_ID = $P{fyId} -2) prevYear ON
	prevYear.FINANCIAL_YEAR_ID = $P{fyId} -2
ORDER BY
	final_query.NO]]>
	</queryString>
	<field name="MAJHEADCOUNT" class="java.lang.Integer">
		<property name="com.jaspersoft.studio.field.label" value="MAJHEADCOUNT"/>
	</field>
	<field name="NUMOFGRANT_GUJ" class="java.lang.String">
		<property name="com.jaspersoft.studio.field.label" value="NUMOFGRANT_GUJ"/>
		<property name="com.jaspersoft.studio.field.tree.path" value="GUJNO"/>
