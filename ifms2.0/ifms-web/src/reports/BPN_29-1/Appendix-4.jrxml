<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="Appendix-4" pageWidth="802" pageHeight="555" orientation="Landscape" whenNoDataType="AllSectionsNoDetail" columnWidth="802" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="308825b8-79e6-47c8-b726-6ad5bc8dc666">
	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="IFMS_V1"/>
	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<parameter name="fyId" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
       final_query.BPN_ID,
    BUDGET.FN_CONVERT_STRING_TO_SP_CAMEL(final_query.BPN_NAME) BPN_NAME,
       final_query.BPN_NAME_GUJ,
       final_query.MAJORHEAD_ID,
       final_query.MAJORHEAD_CODE,
       final_query.MAJORHEAD_NAME,
       final_query.MAJORHEAD_NAME_GUJ,
       REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(final_query.MAJORHEAD_CODE_GUJ,'1','૧'), '2', '૨'),'3','૩'), 
                '4','૪'),'5','૫'),'6','૬'),'7','૭'),'8','૮'),'9','૯'),'0','૦') AS "MAJORHEAD_CODE_GUJ", 
    --   final_query.BUD_EST,
       final_query.DEMAND_ID,
    --   final_query.REV_EXP,
    --   final_query.MBE_EXP,
    final_query.BE_FST,
    final_query.BE_SEC,
    final_query.BE_THR,
    final_query.REV_FST,
    final_query.REV_SEC,
    final_query.REV_THR,
    final_query.MBE_FST,
    final_query.MBE_SEC,
    final_query.MBE_THR,
       nextYear.FY AS BUD_EST_YEAR_ENG,
       nextYear.FY_GUJ AS BUD_EST_YEAR_GUJ,
       currYear.FY AS MOD_REV_YEAR_ENG,
       currYear.FY_GUJ AS MOD_REV_YEAR_GUJ,
       prevYear.FY AS ACC_YEAR_ENG,
       prevYear.FY_GUJ AS ACC_YEAR_GUJ
FROM
       (
       SELECT
             COALESCE(abc.BPN_ID , rev.BPN_ID, mbe.BPN_ID,wipl.BPN_ID,nwlt.BPN_ID,rewipl.BPN_ID,renwlt.BPN_ID,mbewipl.BPN_ID,mbenwlt.BPN_ID) AS BPN_ID, COALESCE(abc.BPN_NAME, rev.BPN_NAME, mbe.BPN_NAME,wipl.BPN_NAME,nwlt.BPN_NAME,rewipl.BPN_NAME,renwlt.BPN_NAME,mbewipl.BPN_NAME,mbenwlt.BPN_NAME)AS BPN_NAME, COALESCE(abc.BPN_NAME_GUJ , rev.BPN_NAME_GUJ, mbe.BPN_NAME_GUJ,wipl.BPN_NAME_GUJ,nwlt.BPN_NAME_GUJ,rewipl.BPN_NAME_GUJ,renwlt.BPN_NAME_GUJ,mbewipl.BPN_NAME_GUJ,mbenwlt.BPN_NAME_GUJ) AS BPN_NAME_GUJ, COALESCE(abc.MAJORHEAD_ID, rev.MAJORHEAD_ID, mbe.MAJORHEAD_ID, wipl.MAJORHEAD_ID,nwlt.MAJORHEAD_ID,rewipl.MAJORHEAD_ID,renwlt.MAJORHEAD_ID,mbewipl.MAJORHEAD_ID,mbenwlt.MAJORHEAD_ID)AS MAJORHEAD_ID, COALESCE(abc.MAJORHEAD_CODE, rev.MAJORHEAD_CODE, mbe.MAJORHEAD_CODE, wipl.MAJORHEAD_CODE,nwlt.MAJORHEAD_CODE,rewipl.MAJORHEAD_CODE,renwlt.MAJORHEAD_CODE,mbewipl.MAJORHEAD_CODE,mbenwlt.MAJORHEAD_CODE) AS MAJORHEAD_CODE, COALESCE(abc.MAJORHEAD_NAME, rev.MAJORHEAD_NAME, mbe.MAJORHEAD_NAME, wipl.MAJORHEAD_NAME,nwlt.MAJORHEAD_NAME,rewipl.MAJORHEAD_NAME,renwlt.MAJORHEAD_NAME,mbewipl.MAJORHEAD_NAME,mbenwlt.MAJORHEAD_NAME) AS MAJORHEAD_NAME, COALESCE(abc.MAJORHEAD_NAME_GUJ, rev.MAJORHEAD_NAME_GUJ, mbe.MAJORHEAD_NAME_GUJ, wipl.MAJORHEAD_NAME_GUJ,nwlt.MAJORHEAD_NAME_GUJ,rewipl.MAJORHEAD_NAME_GUJ,renwlt.MAJORHEAD_NAME_GUJ,mbewipl.MAJORHEAD_NAME_GUJ,mbenwlt.MAJORHEAD_NAME_GUJ) AS MAJORHEAD_NAME_GUJ, COALESCE(abc.MAJORHEAD_CODE_GUJ, rev.MAJORHEAD_CODE_GUJ, mbe.MAJORHEAD_CODE_GUJ, wipl.MAJORHEAD_CODE_GUJ,nwlt.MAJORHEAD_CODE_GUJ,rewipl.MAJORHEAD_CODE_GUJ,renwlt.MAJORHEAD_CODE_GUJ,mbewipl.MAJORHEAD_CODE_GUJ,mbenwlt.MAJORHEAD_CODE_GUJ) AS MAJORHEAD_CODE_GUJ , abc.BUD_EST AS BE_FST, wipl.BUD_EST_WIP AS BE_SEC,nwlt.BUD_EST_NWL AS BE_THR,
           --  COALESCE(abc.BUD_EST, wipl.BUD_EST_WIP,nwlt.BUD_EST_NWL)AS Bud_Est,
            COALESCE(abc.DEMAND_ID, rev.DEMAND_ID, mbe.DEMAND_ID, wipl.DEMAND_ID,nwlt.DEMAND_ID,rewipl.DEMAND_ID,renwlt.DEMAND_ID,mbewipl.DEMAND_ID,mbenwlt.DEMAND_ID)AS DEMAND_ID,
            rev.REV_EXP AS REV_FST,rewipl.REV_EXP_WIP AS REV_SEC,renwlt.REV_EXP_NWL AS REV_THR,
             mbe.MBE_EXP AS MBE_FST,mbewipl.MBE_EXP_WIP AS MBE_SEC,mbenwlt.MBE_EXP_NWL AS MBE_THR
      --    COALESCE(rev.REV_EXP,rewipl.REV_EXP_WIP,renwlt.REV_EXP_NWL) AS REV_EXP,
      --    COALESCE( mbe.MBE_EXP,mbewipl.MBE_EXP_WIP,mbenwlt.MBE_EXP_NWL)AS MBE_EXP
       FROM
             (
             SELECT
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID, ROUND(SUM(dtl.PA_CSS), 2) AS BUD_EST
             FROM
                    BUDGET.TBUD_PUB_HDR hdr, BUDGET.TBUD_PUB_DTL dtl, MASTER_V1.MS_MAJORHEAD mh, BUDGET.TBUD_PUB_NC_HDR NCHDR, MASTER_V1.MS_BPN bpn
             WHERE
                    hdr.MAJORHEAD_ID = mh.MAJORHEAD_ID
                    AND hdr.TBUD_PUB_HDR_ID = dtl.TBUD_PUB_HDR_ID
                    AND hdr.TBUD_PUB_HDR_ID = NCHDR.TBUD_PUB_HDR_ID
                    AND bpn.BPN_ID = hdr.BPN_ID
                    AND hdr.IS_REVISED = 0
                    AND hdr.APP_TYPE_ID IN (425, 426, 427, 428, 429)
                 --   AND NCHDR.SCHEME_TYPE_ID IN (624, 625)
                    AND HDR.IS_REVENUE_CAPITAL IN(5, 6)
                    AND dtl.IS_CSS = 10
                    AND hdr.ACTIVE_STATUS =1
                    AND hdr.FINANCIAL_YEAR_ID = $P{fyId}
             GROUP BY
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID
             ORDER BY
                    bpn.BPN_NAME, hdr.DEMAND_ID) abc
       FULL OUTER JOIN (
             SELECT
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID, ROUND(SUM(tpwl.PA_CSS ), 2) AS BUD_EST_WIP
             FROM
                    BUDGET.TBUD_PUB_HDR hdr, MASTER_V1.MS_MAJORHEAD mh, BUDGET.TBUD_PUB_NC_HDR NCHDR, BUDGET.TBUD_PUB_WIP_LST tpwl , MASTER_V1.MS_BPN bpn
             WHERE
                    hdr.MAJORHEAD_ID = mh.MAJORHEAD_ID
                    AND hdr.TBUD_PUB_HDR_ID = NCHDR.TBUD_PUB_HDR_ID
                    AND hdr.TBUD_PUB_HDR_ID = tpwl.TBUD_PUB_HDR_ID
                    AND bpn.BPN_ID = hdr.BPN_ID
                    AND hdr.IS_REVISED = 0
                    AND hdr.APP_TYPE_ID IN (425, 426, 427, 428, 429)
                 --   AND NCHDR.SCHEME_TYPE_ID IN (624, 625)
                    AND HDR.IS_REVENUE_CAPITAL IN(5, 6)
                    AND tpwl.IS_CSS = 10
                    AND hdr.ACTIVE_STATUS =1
                    AND hdr.FINANCIAL_YEAR_ID = $P{fyId}
             GROUP BY
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID
             ORDER BY
                    bpn.BPN_NAME, hdr.DEMAND_ID) wipl ON
             abc.BPN_ID = wipl.BPN_ID
             AND abc.MAJORHEAD_ID = wipl.MAJORHEAD_ID
       FULL OUTER JOIN (
             SELECT
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID, ROUND(SUM(nwl.PROV_NEXT_FY_CSS ), 2) AS BUD_EST_NWL
             FROM
                    BUDGET.TBUD_PUB_HDR hdr, MASTER_V1.MS_MAJORHEAD mh, BUDGET.TBUD_PUB_NC_HDR NCHDR, BUDGET.TBUD_PUB_NW_LST nwl , MASTER_V1.MS_BPN bpn
             WHERE
                    hdr.MAJORHEAD_ID = mh.MAJORHEAD_ID
                    AND hdr.TBUD_PUB_HDR_ID = NCHDR.TBUD_PUB_HDR_ID
                    AND hdr.TBUD_PUB_HDR_ID = nwl.TBUD_PUB_HDR_ID
                    AND bpn.BPN_ID = hdr.BPN_ID
                    AND hdr.IS_REVISED = 0
                    AND hdr.APP_TYPE_ID IN (425, 426, 427, 428, 429)
                  --  AND NCHDR.SCHEME_TYPE_ID IN (624, 625)
                    AND HDR.IS_REVENUE_CAPITAL IN(5, 6)
                    AND nwl.IS_CSS = 10
                    AND hdr.ACTIVE_STATUS =1
                    AND hdr.FINANCIAL_YEAR_ID = $P{fyId}
             GROUP BY
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID
             ORDER BY
                    bpn.BPN_NAME, hdr.DEMAND_ID) nwlt ON
                    (abc.BPN_ID = nwlt.BPN_ID OR nwlt.BPN_ID = wipl.BPN_ID)
                    AND (abc.MAJORHEAD_ID = nwlt.MAJORHEAD_ID OR nwlt.MAJORHEAD_ID = wipl.MAJORHEAD_ID)
       FULL OUTER JOIN (
             SELECT
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID, ROUND(SUM(DTL.PROPOSED_AMOUNT), 2) AS REV_EXP
             FROM
                    BUDGET.TBUD_PUB_HDR hdr, MASTER_V1.MS_MAJORHEAD mh, MASTER_V1.MS_BPN bpn, BUDGET.TBUD_PUB_NC_HDR NCHDR, BUDGET.TBUD_PUB_DTL dtl
             WHERE
                    HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
                    AND bpn.BPN_ID = hdr.BPN_ID
                    AND hdr.TBUD_PUB_HDR_ID = NCHDR.TBUD_PUB_HDR_ID
                    AND HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
                    AND hdr.IS_REVISED = 1
                    AND hdr.APP_TYPE_ID IN (425, 426, 427, 428, 429)
                    AND hdr.FINANCIAL_YEAR_ID = $P{fyId} - 1
                    AND dtl.IS_CSS = 10
                    AND hdr.ACTIVE_STATUS =1
                    AND HDR.IS_REVENUE_CAPITAL IN(5, 6)
              --      AND NCHDR.SCHEME_TYPE_ID IN (624, 625)
             GROUP BY
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID
             ORDER BY
                    MH.MAJORHEAD_CODE, bpn.BPN_NAME)rev ON
             (abc.BPN_ID = rev.BPN_ID OR rev.BPN_ID = wipl.BPN_ID OR rev.BPN_ID = nwlt.BPN_ID)
                    AND (abc.MAJORHEAD_ID = rev.MAJORHEAD_ID OR rev.MAJORHEAD_ID = wipl.MAJORHEAD_ID OR rev.MAJORHEAD_ID = nwlt.MAJORHEAD_ID)
       FULL OUTER JOIN (
             SELECT
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID, ROUND(SUM(tpwl.PROPOSED_AMOUNT), 2) AS REV_EXP_WIP
             FROM
                    BUDGET.TBUD_PUB_HDR hdr, MASTER_V1.MS_MAJORHEAD mh, MASTER_V1.MS_BPN bpn, BUDGET.TBUD_PUB_NC_HDR NCHDR, BUDGET.TBUD_PUB_WIP_LST tpwl
             WHERE
                    HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
                    AND bpn.BPN_ID = hdr.BPN_ID
                    AND hdr.TBUD_PUB_HDR_ID = NCHDR.TBUD_PUB_HDR_ID
                    AND HDR.TBUD_PUB_HDR_ID = tpwl.TBUD_PUB_HDR_ID
                    AND hdr.IS_REVISED = 1
                    AND hdr.APP_TYPE_ID IN (425, 426, 427, 428, 429)
                    AND hdr.FINANCIAL_YEAR_ID = $P{fyId} - 1
                    AND tpwl.IS_CSS = 10
                    AND hdr.ACTIVE_STATUS =1
                    AND HDR.IS_REVENUE_CAPITAL IN(5, 6)
                  --  AND NCHDR.SCHEME_TYPE_ID IN (624, 625)
             GROUP BY
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID
             ORDER BY
                    MH.MAJORHEAD_CODE, bpn.BPN_NAME)rewipl ON
             (abc.BPN_ID = rewipl.BPN_ID OR rewipl.BPN_ID = wipl.BPN_ID OR rewipl.BPN_ID = nwlt.BPN_ID OR rewipl.BPN_ID=rev.BPN_ID)
                    AND (abc.MAJORHEAD_ID = rewipl.MAJORHEAD_ID OR rewipl.MAJORHEAD_ID = wipl.MAJORHEAD_ID OR rewipl.MAJORHEAD_ID = nwlt.MAJORHEAD_ID OR rewipl.MAJORHEAD_ID=rev.MAJORHEAD_ID)      
             FULL OUTER JOIN (
             SELECT
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID, ROUND(SUM(nwl.PROVISION_NEXT_FY ), 2) AS REV_EXP_NWL
             FROM
                    BUDGET.TBUD_PUB_HDR hdr, MASTER_V1.MS_MAJORHEAD mh, MASTER_V1.MS_BPN bpn, BUDGET.TBUD_PUB_NC_HDR NCHDR, BUDGET.TBUD_PUB_NW_LST nwl
             WHERE
                    HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
                    AND bpn.BPN_ID = hdr.BPN_ID
                    AND hdr.TBUD_PUB_HDR_ID = NCHDR.TBUD_PUB_HDR_ID
                    AND HDR.TBUD_PUB_HDR_ID = nwl.TBUD_PUB_HDR_ID
                    AND hdr.IS_REVISED = 1
                    AND hdr.APP_TYPE_ID IN (425, 426, 427, 428, 429)
                    AND hdr.FINANCIAL_YEAR_ID = $P{fyId} - 1
                    AND nwl.IS_CSS = 10
                    AND hdr.ACTIVE_STATUS =1
                    AND HDR.IS_REVENUE_CAPITAL IN(5, 6)
                   -- AND NCHDR.SCHEME_TYPE_ID IN (624, 625)
             GROUP BY
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID
             ORDER BY
                    MH.MAJORHEAD_CODE, bpn.BPN_NAME)renwlt ON
             (abc.BPN_ID = renwlt.BPN_ID OR renwlt.BPN_ID = wipl.BPN_ID OR renwlt.BPN_ID = nwlt.BPN_ID OR renwlt.BPN_ID=rev.BPN_ID OR renwlt.BPN_ID = rewipl.BPN_ID)
                    AND (abc.MAJORHEAD_ID = renwlt.MAJORHEAD_ID OR renwlt.MAJORHEAD_ID = wipl.MAJORHEAD_ID OR renwlt.MAJORHEAD_ID = nwlt.MAJORHEAD_ID OR renwlt.MAJORHEAD_ID=rev.MAJORHEAD_ID OR renwlt.MAJORHEAD_ID = rewipl.MAJORHEAD_ID)
       FULL OUTER JOIN (
             SELECT
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID, ROUND(SUM(dtl.PA_CSS), 2) AS MBE_EXP
             FROM
                    BUDGET.TBUD_PUB_HDR hdr, MASTER_V1.MS_MAJORHEAD mh, MASTER_V1.MS_BPN bpn, BUDGET.TBUD_PUB_NC_HDR NCHDR, BUDGET.TBUD_PUB_DTL dtl
             WHERE
                    HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
                    AND bpn.BPN_ID = hdr.BPN_ID
                    AND hdr.TBUD_PUB_HDR_ID = NCHDR.TBUD_PUB_HDR_ID
                    AND HDR.TBUD_PUB_HDR_ID = DTL.TBUD_PUB_HDR_ID
                    AND hdr.IS_REVISED <> 1
                     AND dtl.IS_CSS = 10
                    AND hdr.ACTIVE_STATUS =1
                    AND hdr.APP_TYPE_ID IN (425, 426, 427, 428, 429)
                    AND hdr.FINANCIAL_YEAR_ID = $P{fyId} - 1
                    AND HDR.IS_REVENUE_CAPITAL IN(5, 6)
                 --   AND NCHDR.SCHEME_TYPE_ID IN (624, 625)
                   
             GROUP BY
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID
             ORDER BY
                    MH.MAJORHEAD_CODE, bpn.BPN_NAME)mbe ON
                    (abc.BPN_ID = mbe.BPN_ID OR mbe.BPN_ID = wipl.BPN_ID OR mbe.BPN_ID = nwlt.BPN_ID OR mbe.BPN_ID=rev.BPN_ID OR mbe.BPN_ID = rewipl.BPN_ID OR mbe.BPN_ID=renwlt.BPN_ID)
                    AND (abc.MAJORHEAD_ID = mbe.MAJORHEAD_ID OR mbe.MAJORHEAD_ID = wipl.MAJORHEAD_ID OR mbe.MAJORHEAD_ID = nwlt.MAJORHEAD_ID OR mbe.MAJORHEAD_ID=rev.MAJORHEAD_ID OR mbe.MAJORHEAD_ID = rewipl.MAJORHEAD_ID OR mbe.MAJORHEAD_ID=renwlt.MAJORHEAD_ID)               
       FULL OUTER JOIN (
             SELECT
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID, ROUND(SUM(tpwl.PA_CSS), 2) AS MBE_EXP_WIP
             FROM
                    BUDGET.TBUD_PUB_HDR hdr, MASTER_V1.MS_MAJORHEAD mh, MASTER_V1.MS_BPN bpn, BUDGET.TBUD_PUB_NC_HDR NCHDR, BUDGET.TBUD_PUB_WIP_LST tpwl 
             WHERE
                    HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
                    AND bpn.BPN_ID = hdr.BPN_ID
                    AND hdr.TBUD_PUB_HDR_ID = NCHDR.TBUD_PUB_HDR_ID
                    AND HDR.TBUD_PUB_HDR_ID = tpwl.TBUD_PUB_HDR_ID
                    AND hdr.IS_REVISED <> 1
                     AND tpwl.IS_CSS = 10
                    AND hdr.ACTIVE_STATUS =1
                    AND hdr.APP_TYPE_ID IN (425, 426, 427, 428, 429)
                    AND hdr.FINANCIAL_YEAR_ID = $P{fyId} - 1
                    AND HDR.IS_REVENUE_CAPITAL IN(5, 6)
               --     AND NCHDR.SCHEME_TYPE_ID IN (624, 625)
                   
             GROUP BY
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID
             ORDER BY
                    MH.MAJORHEAD_CODE, bpn.BPN_NAME)mbewipl ON
                    (abc.BPN_ID = mbewipl.BPN_ID OR mbewipl.BPN_ID = wipl.BPN_ID OR mbewipl.BPN_ID = mbewipl.BPN_ID OR mbewipl.BPN_ID=rev.BPN_ID OR mbewipl.BPN_ID = rewipl.BPN_ID OR mbewipl.BPN_ID=renwlt.BPN_ID OR mbewipl.BPN_ID = mbe.BPN_ID)
                    AND (abc.MAJORHEAD_ID = mbewipl.MAJORHEAD_ID OR mbewipl.MAJORHEAD_ID = wipl.MAJORHEAD_ID OR mbewipl.MAJORHEAD_ID = nwlt.MAJORHEAD_ID OR mbewipl.MAJORHEAD_ID=rev.MAJORHEAD_ID OR mbewipl.MAJORHEAD_ID = rewipl.MAJORHEAD_ID OR mbewipl.MAJORHEAD_ID=renwlt.MAJORHEAD_ID OR mbewipl.MAJORHEAD_ID=mbe.MAJORHEAD_ID)        
       
       FULL OUTER JOIN (
             SELECT
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID, ROUND(SUM(nwl.PROV_NEXT_FY_CSS ), 2) AS MBE_EXP_NWL
             FROM
                    BUDGET.TBUD_PUB_HDR hdr, MASTER_V1.MS_MAJORHEAD mh, MASTER_V1.MS_BPN bpn, BUDGET.TBUD_PUB_NC_HDR NCHDR, BUDGET.TBUD_PUB_NW_LST nwl
             WHERE
                    HDR.MAJORHEAD_ID = MH.MAJORHEAD_ID
                    AND bpn.BPN_ID = hdr.BPN_ID
                    AND hdr.TBUD_PUB_HDR_ID = NCHDR.TBUD_PUB_HDR_ID
                    AND HDR.TBUD_PUB_HDR_ID = nwl.TBUD_PUB_HDR_ID
                    AND hdr.IS_REVISED <> 1
                    AND nwl.IS_CSS = 10
                    AND hdr.ACTIVE_STATUS =1
                    AND hdr.APP_TYPE_ID IN (425, 426, 427, 428, 429)
                    AND hdr.FINANCIAL_YEAR_ID = $P{fyId} - 1
                    AND HDR.IS_REVENUE_CAPITAL IN(5, 6)
                  --  AND NCHDR.SCHEME_TYPE_ID IN (624, 625)
                    
             GROUP BY
                    bpn.BPN_ID , bpn.BPN_NAME, bpn.BPN_NAME_GUJ , hdr.MAJORHEAD_ID, MH.MAJORHEAD_CODE, mh.MAJORHEAD_NAME, mh.MAJORHEAD_NAME_GUJ, mh.MAJORHEAD_CODE_GUJ, hdr.DEMAND_ID
             ORDER BY
                    MH.MAJORHEAD_CODE, bpn.BPN_NAME)mbenwlt ON
                    (abc.BPN_ID = mbenwlt.BPN_ID OR mbenwlt.BPN_ID = wipl.BPN_ID OR mbenwlt.BPN_ID = nwlt.BPN_ID OR mbenwlt.BPN_ID=rev.BPN_ID OR mbenwlt.BPN_ID = rewipl.BPN_ID OR mbenwlt.BPN_ID=renwlt.BPN_ID OR mbenwlt.BPN_ID=mbe.BPN_ID OR mbenwlt.BPN_ID=mbewipl.BPN_ID)
                    AND (abc.MAJORHEAD_ID = mbenwlt.MAJORHEAD_ID OR mbenwlt.MAJORHEAD_ID = wipl.MAJORHEAD_ID OR mbenwlt.MAJORHEAD_ID = nwlt.MAJORHEAD_ID OR mbenwlt.MAJORHEAD_ID=rev.MAJORHEAD_ID OR mbenwlt.MAJORHEAD_ID = rewipl.MAJORHEAD_ID OR mbenwlt.MAJORHEAD_ID=renwlt.MAJORHEAD_ID OR mbenwlt.MAJORHEAD_ID=mbe.MAJORHEAD_ID OR mbenwlt.MAJORHEAD_ID = mbewipl.MAJORHEAD_ID)
       ORDER BY
             MAJORHEAD_CODE, BPN_NAME )final_query
JOIN (
       SELECT
             mfy.FY, mfy.FY_GUJ, mfy.FINANCIAL_YEAR_ID
       FROM
             MASTER_V1.MS_FINANCIAL_YEAR mfy
       WHERE
             mfy.FINANCIAL_YEAR_ID = $P{fyId}) nextYear ON
       nextYear.FINANCIAL_YEAR_ID = $P{fyId}
JOIN (
       SELECT
             mfy.FY, mfy.FY_GUJ, mfy.FINANCIAL_YEAR_ID
       FROM
             MASTER_V1.MS_FINANCIAL_YEAR mfy
       WHERE
             mfy.FINANCIAL_YEAR_ID = $P{fyId} - 1) currYear ON
       currYear.FINANCIAL_YEAR_ID = $P{fyId} - 1
JOIN (
       SELECT
             mfy.FY, mfy.FY_GUJ, mfy.FINANCIAL_YEAR_ID
       FROM
             MASTER_V1.MS_FINANCIAL_YEAR mfy
       WHERE
             mfy.FINANCIAL_YEAR_ID = $P{fyId} - 2) prevYear ON
       prevYear.FINANCIAL_YEAR_ID = $P{fyId} - 2]]>
	</queryString>
