	<queryString>
		<![CDATA[SELECT DISTINCT ms.SECTORHEAD_NAME,ms.SECTORHEAD_NAME_GUJ ,mm.SECTORHEAD_CODE,
nextYear.FY AS BUD_EST_YEAR_ENG,
      nextYear.FY_GUJ AS BUD_EST_YEAR_GUJ,
                currYear.FY AS MOD_REV_YEAR_ENG,
                currYear.FY_GUJ AS MOD_REV_YEAR_GUJ,
                prevYear.FY AS ACC_YEAR_ENG,
                prevYear.FY_GUJ AS ACC_YEAR_GUJ
FROM MASTER_V1.MS_SECTORHEAD ms LEFT JOIN MASTER_V1.MS_MAJORHEAD mm ON ms.SECTORHEAD_ID=mm.SECTORHEAD_CODE
