                BUDGET.TBUD_PUB_RE_HDR tph LEFT JOIN  
                BUDGET.TBUD_PUB_RE_DTL tprd ON tph.TBUD_PUB_RE_HDR_ID =tprd.TBUD_PUB_RE_HDR_ID 
               
