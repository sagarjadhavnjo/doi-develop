	<queryString language="SQL">
		<![CDATA[SELECT ms.SECTORHEAD_NAME,ms.SECTORHEAD_NAME_GUJ,mss.SUB_SECTOR_NAME,mss.SUB_SECTOR_NAME_GUJ,
mm.MAJORHEAD_NAME,mm.MAJORHEAD_NAME_GUJ ,mm.MAJORHEAD_CODE,
  nextYear.FY AS BUD_EST_YEAR_ENG,
      nextYear.FY_GUJ AS BUD_EST_YEAR_GUJ,
                currYear.FY AS MOD_REV_YEAR_ENG,
                currYear.FY_GUJ AS MOD_REV_YEAR_GUJ,
                prevYear.FY AS ACC_YEAR_ENG,
                prevYear.FY_GUJ AS ACC_YEAR_GUJ,
  REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(mm.MAJORHEAD_CODE,'1','૧'), '2', '૨'),'3','૩'),
'4','૪'),'5','૫'),'6','૬'),'7','૭'),'8','૮'),'9','૯'),'0','૦') AS MAJORHEAD_CODE_GUJ
FROM MASTER_V1.MS_MAJORHEAD mm LEFT JOIN MASTER_V1.MS_SECTORHEAD ms ON mm.SECTORHEAD_CODE=ms.SECTORHEAD_ID
LEFT  JOIN
MASTER_V1.MS_SUB_SECTOR mss ON mm.SUB_SECTOR_CODE=mss.SUB_SECTOR_ID
JOIN (SELECT mfy.FY, mfy.FY_GUJ, mfy.FINANCIAL_YEAR_ID FROM MASTER_V1.MS_FINANCIAL_YEAR mfy WHERE mfy.FINANCIAL_YEAR_ID = $P{fyId}) nextYear ON nextYear.FINANCIAL_YEAR_ID=$P{fyId}
JOIN (SELECT mfy.FY, mfy.FY_GUJ, mfy.FINANCIAL_YEAR_ID FROM MASTER_V1.MS_FINANCIAL_YEAR mfy WHERE mfy.FINANCIAL_YEAR_ID = $P{fyId}-1) currYear ON currYear.FINANCIAL_YEAR_ID=$P{fyId}-1
JOIN (SELECT mfy.FY, mfy.FY_GUJ, mfy.FINANCIAL_YEAR_ID FROM MASTER_V1.MS_FINANCIAL_YEAR mfy WHERE mfy.FINANCIAL_YEAR_ID = $P{fyId}-2) prevYear ON prevYear.FINANCIAL_YEAR_ID=$P{fyId}-2  WHERE mm.MAJORHEAD_TYPE_ID =3 AND mm.MAJORHEAD_CODE= $P{major} 
ORDER BY MAJORHEAD_CODE]]>
	</queryString>
