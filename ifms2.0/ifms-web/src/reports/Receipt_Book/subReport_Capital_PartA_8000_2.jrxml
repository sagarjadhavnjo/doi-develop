(
 SELECT 
                abc.SUBMAJORHEAD_CODE, 
                abc.SUBMAJORHEAD_NAME,
                abc.SUBMAJORH_NAME_GUJ,
                abc.MINORHEAD_CODE, 
                abc.MINORHEAD_NAME, 
