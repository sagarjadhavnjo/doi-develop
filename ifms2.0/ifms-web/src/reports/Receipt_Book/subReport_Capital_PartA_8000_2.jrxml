                ROUND(sum(abc.ACC),2) AS ACC_BUD_EST,
                ROUND(sum(abc.DSB),2) AS ACC_BUD_EST_DA,
                abc.DEMAND_ID, 
                abc.MAJORHEAD_ID, 
